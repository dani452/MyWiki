{% extends 'base.html.twig' %}

{% block title %}Lewiki liste d'article{% endblock %}

{% block body %}


  <header class="header_title">
    <nav class="nav_title">
      <h1 class="article_title"><span class="blue">L</span><span class="red">e</span><span class="yellow">w</span><span class="blue">i</span><span class="green">k</span><span class="red">i</span>
      </h1>
      <a href="{{ path('home') }}" class="button_article" role="button">Retour</a>
    </nav>
</header>
  <div class="content_article">
    <a id="button"></a>
    {% for titles in titles %}
    {{ titles|raw }}<br>
    {% endfor %}
    
  </div>


  {% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('list') }}
  {% endblock %}

{% endblock %}